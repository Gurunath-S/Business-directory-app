{
  "expo": {
    "name": "bda",
    "slug": "bda",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bda.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "splashMode": "cover",
      "preload": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bda.png",
        "backgroundColor": "#808080"
      },
      "package": "com.guru.bda"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/facebook-verified.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "348112de-0daa-4ef1-b8cc-77c30aaa2eab"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/348112de-0daa-4ef1-b8cc-77c30aaa2eab"
    }
  }
}
